{
  "json-form-item": {
    "prefix": "json-form-item",
    "body": [
      "{",
      "  type: '${1|input,select,textArea|}',",
      "  name: '$2',",
      "  label:'$3',",
      "  rules:[{required:true}]",
      "},"
    ]
  },
